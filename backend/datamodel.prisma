enum Permission {
  ADMIN
  USER
  BATHROOMCREATE
  BATHROOMUPDATE
  BATHROOMDELETE
  PERMISSIONUPDATE
}

type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  resetToken: String
  resetTokenExpiry: String
  permissions: [Permission]
}

type Bathroom {
  id: ID! @unique
  title: String!
  description: String!
  address: String!
  lat: Float
  lng: Float
  image: String
  largeImage: String
  user: User!
}
